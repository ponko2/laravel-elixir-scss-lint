{
  "name": "laravel-elixir-scss-lint",
  "description": "Laravel Elixir SCSS-Lint Extension",
  "version": "2.0.0-1",
  "author": "Takahito Nakano",
  "bugs": {
    "url": "https://github.com/ponko2/laravel-elixir-scss-lint/issues"
  },
  "dependencies": {
    "events": "^1.1.1",
    "gulp-scss-lint": "^0.4.0",
    "gulp-util": "^3.0.7",
    "map-stream": "^0.1.0",
    "pluralize": "^3.0.0",
    "string-length": "^1.0.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "^3.9.1",
    "eslint-config-ponko2": "^8.0.0"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/ponko2/laravel-elixir-scss-lint",
  "keywords": [
    "elixir",
    "gulp",
    "laravel",
    "scss-lint"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "peerDependencies": {
    "laravel-elixir": ">=6.0.0-11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ponko2/laravel-elixir-scss-lint.git"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps inline",
    "lint": "eslint .",
    "prepublish": "npm run --if-present build",
    "watch": "babel src --out-dir lib --watch --source-maps inline"
  }
}
